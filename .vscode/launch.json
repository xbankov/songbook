  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: FastAPI",
      "type": "debugpy",
      "request": "launch",
      "cwd": "${workspaceFolder}/backend/src",
      "module": "uvicorn",
      "args": ["main:app", "--reload"],
      "jinja": true
    },
    {
      "name": "bookmarks2chordpro",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/bookmarks2chordpro.py",
      "console": "integratedTerminal"
    },
    {
      "name": "Build Songbook",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/build_songbook.py",
      "console": "integratedTerminal"
    }
  ]
}
